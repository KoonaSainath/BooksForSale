@{
    ViewData["Title"] = "Manage orders";
}

<div class="container">
    <div class="mt-4">
        <h5>@ViewData["Title"]</h5>
    </div>
    <table class="table table-bordered table-striped mt-4" id="tableOrders">
        <tr>
            <th>Order id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone number</th>
            <th>Status</th>
            <th></th>
        </tr>
    </table>
</div>

@section Scripts{
    <script src="~/js/ManageOrders.js"></script>
}