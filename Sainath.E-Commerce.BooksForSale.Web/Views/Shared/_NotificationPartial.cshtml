@if (TempData[GenericConstants.NOTIFICATION_MESSAGE_KEY] != null && TempData[GenericConstants.NOTIFICATION_MESSAGE_KEY].ToString().ToLower().Contains("fail"))
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script>
        toastr.error('@TempData[GenericConstants.NOTIFICATION_MESSAGE_KEY]');
    </script>
}
else if(TempData[GenericConstants.NOTIFICATION_MESSAGE_KEY] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script>
        toastr.success('@TempData[GenericConstants.NOTIFICATION_MESSAGE_KEY]');
    </script>
}